                                    <option value="Rencontre privée">Rencontre privée</option>
                                    <option value="Rencontre groupée">Rencontre groupée</option>
                                    <option value="Autre">Autre</option>
                                </select>
                            </div>
                            <div class="col-md-3 mb-2 mb-md-0">
                                <input type="text" id="event-place" value="" name="lieu" class="form-control"
                                    placeholder="Lieu, ville...">
                            </div>
                            <div class="col-md-3">
                                <button type="submit" class="btn btn-theme w-100 text-capitalize">filtrer <i
                                        class="fa-solid fa-filter"></i></button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="col-12">
                <div class="container">
                    <div class="row mb-3">
                    </div>
                    <div class="row" id="events-list">
                        {{#each event}}
                        <div class="col-md-6 col-lg-4 event-content">
                            <div class="event-card mx-1 mb-3">
                                <div class="event-img cursor-pointer">
                                    <span class="verify-badge bg-success"><i class="fa-solid fa-check"></i>
                                        vérifié</span>
                                    <a href="/users/events/{{this._id}}">
                                        <img class="d-block w-100"
                                            src="{{ this.image }}"
                                            alt=" {{this.event_name}}">
                                    </a>
                                </div>
                                <div class="event-info">
                                    <h3 class="event-title">
                                        <a href="/users/events/{{this._id}}">{{this.event_name}}</a>
                                    </h3>
                                    <ul>
                                        <li><i class="fa-solid fa-calendar"></i> {{ this.date_due }} <small
                                                class="text-muted">GMT</small></li>
                                        <li><i class="fa-solid fa-location-dot"></i>{{this.place}}</li>
                                        <li><i class="fa-solid fa-money-bill"></i>{{this.cost}}</li>
                                        <hr>
                                        <li class="pl-0">Concert-Spectacle</li>
                                    </ul>
                                    <div class="footer">
                                        <a href="/users/events/{{this._id}}">Acheter tickets</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
