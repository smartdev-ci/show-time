    <div class="container mt-3 mb-5">
        <div class="row">
            <div class="col-12 mb-3">
                <form id="filter-form" action="#" method="GET">
                    <div class="container add-box-shadow py-2 rounded-10">
                        <div class="row">
                            <div class="col-md-3 mb-2 mb-md-0">
                                <input type="text" name="recherche" class="form-control"
                                    placeholder=" Recherchez un événement..." value="">
                            </div>
                            <div class="col-md-3 mb-2 mb-md-0">
                                <select name="categorie" class="form-control">
                                    <option selected class="text-capitalize">Catégories</option>
                                    <option value="Concert-Spectacle">Concert-Spectacle</option>
                                    <option value="Sport">Sport</option>
                                    <option value="Diner Gala">Diner Gala</option>
                                    <option value="Soirée-Party">Soirée-Party</option>
                                    <option value="Tourisme">Tourisme</option>
                                    <option value="Formation">Formation</option>
